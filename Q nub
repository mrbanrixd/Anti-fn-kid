print("[*] cloud syncing...")
time.sleep(1)
print("[*] waiting for API response...")
time.sleep(3)
print("[*] connection successful")
time.sleep(0.6)
print("[*] server status:active")
time.sleep(0.6)
print("[$] recent logs:null\n")
time.sleep(1.2)
