# Connecting to server in random %
ran1 = random.randint(5,10)
print("[*] connecting...             ",ran1,"%")
time.sleep(0.3)
ran2 = random.randint(15,20)
print("[*] connecting...             ",ran2,"%")
time.sleep(0.3)
